+  crc = crc32(crc, (unsigned char const *) "$Id: hcache.c,v 3.11 2005/09/22 16:38:08 brendan Exp $", mutt_strlen("$Id: hcache.c,v 3.11 2005/09/22 16:38:08 brendan Exp $"));
 
