     msg->content = pbody;
-    
-    ctype = safe_strdup (ContentType);
+
+    if (!(ctype = safe_strdup (ContentType)))
+      ctype = safe_strdup ("text/plain");
     mutt_parse_content_type (ctype, msg->content);
     FREE (&ctype);
     msg->content->unlink = 1;
